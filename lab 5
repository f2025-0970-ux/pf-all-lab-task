#include <iostream>
using namespace std;

int main() {
    int n, i = 2, flag = 0;

    cout << "Enter a number: ";
    cin >> n;

    if (n <= 1) {
        cout << "Not Prime";
        return 0;
    }

    while (i <= n / 2) {
        if (n % i == 0) {
            flag = 1;
            break;
        }
        i++;
    }

    if (flag == 0) {
        cout << "Prime";
    } else {
        cout << "Not Prime";
    }

return0;
}